{
  "AllowedFileTypes": [ "xml" ],
  
  "Dispatch": {
    "MaxParallelism": 8,
    "MaxRetries": 3,
    "RetryDelayMs": 200
  },

  "ConnectionStrings": {
    "DispatchDatabase": "Server=localhost\\SQLEXPRESS;Database=TransactionDispatchDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "Logging": {
    "Console": {
      "TimestampFormat": "HH:mm:ss ",
      "IncludeScopes": false
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
